{
    "output_format": "markdown",
    "content_filter_choice": "pruning",

    "browser_config": {
        "headless": true,
        "verbose": true,
        "text_mode": true
    },
    "crawler_run_config": {
        "only_text": true,
        "cache_mode": "BYPASS",
        "check_robots_txt": true,
        "exclude_external_links": true,
        "excluded_tags": ["script", "style", "form", "header", "footer"],
        "remove_overlay_elements": true,
        "process_iframes": false,
        "verbose": true,
        "stream": true
    },
    "markdown_generator_config": {
        "options": {
            "ignore_links": true,
            "ignore_images": true,
            "escape_html": false
        }
    },
    "pruning_content_filter_config": {
        "threshold": 0.5,
        "threshold_type": "dynamic"
    }
}